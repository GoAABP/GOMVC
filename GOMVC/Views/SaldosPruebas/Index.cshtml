@model IEnumerable<GOMVC.Models.SaldoPrueba>

<!DOCTYPE html>
<html>
<head>
    <title>Saldos Pruebas</title>
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css">
</head>
<body>
    <div class="container">
        <h1 class="mt-5">Saldos Pruebas Data</h1>
        <table class="table table-striped mt-3">
            <thead>
                <tr>
                    <th>ID</th>
                    <th>Name</th>
                    <th>Description</th>
                    <th>Created Date</th>
                    <th>Is Active</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>@item.Id</td>
                        <td>@item.Name</td>
                        <td>@item.Description</td>
                        <td>@item.CreatedDate</td>
                        <td>@item.IsActive</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</body>
</html>
